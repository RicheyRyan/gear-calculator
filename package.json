{
  "name": "gear-calculator",
  "version": "0.1.0",
  "scripts": {
    "start": "concurrently -c --kill-others-on-fail 'npm:watch-*'",
    "build": "yarn build-reason && yarn build-html",
    "watch-html": "parcel src/index.html",
    "build-html": "parcel build src/index.html",
    "watch-reason": "bsb -make-world -w",
    "build-reason": "bsb -make-world",
    "clean-reason": "bsb -clean-world",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "BuckleScript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@jsiebern/bs-material-ui": "^1.0.0",
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.2",
    "@mscharley/bs-material-ui-icons": "^0.6.1",
    "global": "^4.3.2",
    "re-classnames": "^3.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reason-react": ">=0.4.0"
  },
  "devDependencies": {
    "@jsiebern/bs-material-ui-ppx": "^1.0.2",
    "bs-platform": "^5.0.0",
    "bsb-js": "^1.1.7",
    "concurrently": "^4.1.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-bundle-visualiser": "^1.2.0"
  }
}
